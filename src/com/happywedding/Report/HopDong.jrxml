     KhachHang."HoTenChuRe" AS KhachHang_HoTenChuRe,
     NhanVien."MaNV" AS NhanVien_MaNV,
     NhanVien."HoTen" AS NhanVien_HoTen,
     NhanVien."NgaySinh" AS NhanVien_NgaySinh,
